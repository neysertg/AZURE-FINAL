@model IEnumerable<AZURE_FINAL.Models.Pelicula>

@{
    ViewData["Title"] = "Lista de peliculas";
}
<br>
<br>

<h1>LISTA DE ASISTENCIAS</h1>

<p>
    <button class="btn btn-primary"><a asp-action="Create">Ingresar Estudiante</a></button>
</p>

<table class="table table-dark table-hover">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Id)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Nombre)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Ape_paterno)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Ape_materno)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Curso)
            </th>
            <th></th>
        </tr>
    </thead>
    
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Nombre)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Ape_materno)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Ape_paterno)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Curso)
            </td>
            <td>
                @Html.ActionLink("Edit", "Editar", new { id=item.Id }) |
                @Html.ActionLink("Details", "Detalles", new { id = item.Id }) |
                @Html.ActionLink("Delete", "Delete", new { id = item.Id })
            </td>
        </tr>
       
}
    </tbody>

</table>
